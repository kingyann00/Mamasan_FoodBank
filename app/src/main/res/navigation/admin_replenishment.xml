<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/admin_replenishment"
    app:startDestination="@id/placeholderFragment">

    <fragment
        android:id="@+id/placeholderFragment"
        android:name="com.example.mamasan_foodbank.ui.main.PlaceholderFragment"
        android:label="fragment_replenishment_list"
        tools:layout="@layout/fragment_replenishment_list" >
        <action
            android:id="@+id/action_placeholderFragment_to_replenishment_search"
            app:destination="@id/replenishment_search" />
    </fragment>
    <fragment
        android:id="@+id/replenishment_search"
        android:name="com.example.mamasan_foodbank.replenishment_search"
        android:label="replenishment_search"
        tools:layout="@layout/fragment_replenishment_search"/>
</navigation>